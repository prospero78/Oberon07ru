МОДУЛЬ модЧислаЦелые;

ИМПОРТ мОт := модОтладка;

ПРОЦЕДУРА Минимум*(пцЗнач1, пцЗнач2: ЦЕЛОЕ): ЦЕЛОЕ;
   ПЕРЕМ
      лцВыход: ЦЕЛОЕ;
   НАЧАЛО
      мОт.Печать("модЧислаЦелые.Минимум");
      ЕСЛИ пцЗнач1 < пцЗнач2 ТОГДА
         лцВыход := пцЗнач1
      ИНАЧЕ
         лцВыход := пцЗнач2
      КОНЕЦ
      ВЕРНУТЬ лцВыход
   КОНЕЦ Минимум;

КОНЕЦ модЧислаЦелые.
