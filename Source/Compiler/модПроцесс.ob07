(*
    Управляет ходом процесса компиляции
    *)
МОДУЛЬ модПроцесс;

ИМПОРТ мАпи := модАпи,
    мКонс := модКонсоль;

ПРОЦЕДУРА Закончить*(пцКодВых: INTEGER);
  НАЧАЛО
    мКонс.Время_Затрачено;
    мАпи.ExitProcess(пцКодВых)
  КОНЕЦ Закончить;

ПРОЦЕДУРА Выход*(пцКодВых: INTEGER);
    НАЧАЛО
        мКонс.Время_Затрачено;
        мАпи.ExitProcess(пцКодВых)
    КОНЕЦ Выход;

КОНЕЦ модПроцесс.
