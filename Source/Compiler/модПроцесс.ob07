(*
    Управляет ходом процесса компиляции
    *)
МОДУЛЬ модПроцесс;

ИМПОРТ мАпи := API,
    мКонс := модКонсоль;

ПРОЦЕДУРА Закончить*(n: INTEGER);
  НАЧАЛО
    мКонс.Время_Затрачено;
    мАпи.ExitProcess(n)
  КОНЕЦ Закончить;

ПРОЦЕДУРА Выход*(код_вых_: INTEGER);
    НАЧАЛО
        мКонс.Время_Затрачено;
        мАпи.ExitProcess(код_вых_)
    КОНЕЦ Выход;

КОНЕЦ модПроцесс.
